<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="chat">
 	<select id="selectOpenChatting" resultType="chat">
 		select 
 		chat_no as chatNo,
 		chat_title as chatTitle,
 		chat_people as chatPeople,
 		chat_enroll_date as chatEnrollDate,
 		member_nickname as memberNickname from chat
 	</select>
 	<insert id="chatInsert" parameterType="chat">
 		insert into chat values(chat_seq.nextval,#{chatTitle},#{chatPeople},sysdate,#{memberNickname})
 	</insert>
</mapper>
